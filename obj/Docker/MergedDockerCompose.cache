name: dockercompose11862061094755103843
services:
  apigateway:
    build:
      context: C:\Users\Abdelhakim\source\repos\Laraconseil\ApiGateway
      dockerfile: Dockerfile
    container_name: apigateway
    depends_on:
      consul:
        condition: service_started
        required: true
      grower-service:
        condition: service_started
        required: true
      station-service:
        condition: service_started
        required: true
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      CONSUL_HTTP_ADDR: http://consul:8500
    networks:
      OurNetworks-App: null
    ports:
      - mode: ingress
        target: 5130
        published: "5130"
        protocol: tcp
      - mode: ingress
        target: 7277
        published: "7277"
        protocol: tcp
  consul:
    command:
      - agent
      - -dev
      - -client=0.0.0.0
    container_name: consul
    image: consul:1.14.6
    networks:
      OurNetworks-App: null
    ports:
      - mode: ingress
        target: 8500
        published: "8500"
        protocol: tcp
      - mode: ingress
        target: 8600
        published: "8600"
        protocol: udp
  grower-service:
    build:
      context: C:\Users\Abdelhakim\source\repos\Laraconseil\GrowerService
      dockerfile: Dockerfile
    container_name: grower-service
    depends_on:
      consul:
        condition: service_started
        required: true
      sqlserver:
        condition: service_started
        required: true
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      CONSUL_HTTP_ADDR: http://consul:8500
      SERVICE_NAME: grower-service
    networks:
      OurNetworks-App: null
    ports:
      - mode: ingress
        target: 5003
        published: "5003"
        protocol: tcp
  keycloak:
    command:
      - start-dev
    container_name: keycloak
    environment:
      KEYCLOAK_ADMIN: admin
      KEYCLOAK_ADMIN_PASSWORD: admin
    image: quay.io/keycloak/keycloak:20.0.3
    networks:
      OurNetworks-App: null
    ports:
      - mode: ingress
        target: 8080
        published: "8080"
        protocol: tcp
  sqlserver:
    container_name: sqlserver-container
    environment:
      ACCEPT_EULA: "Y"
      SA_PASSWORD: StrongPassword@123
    healthcheck:
      test:
        - CMD-SHELL
        - /opt/mssql-tools/bin/sqlcmd -S localhost -U sa -P StrongPassword@123 -Q 'SELECT 1'
      timeout: 5s
      interval: 20s
      retries: 5
    image: mcr.microsoft.com/mssql/server:2019-latest
    networks:
      OurNetworks-App: null
    ports:
      - mode: ingress
        target: 1433
        published: "1433"
        protocol: tcp
    restart: always
    volumes:
      - type: volume
        source: sql_data
        target: /var/opt/mssql
        volume: {}
  station-service:
    build:
      context: C:\Users\Abdelhakim\source\repos\Laraconseil\StationService
      dockerfile: Dockerfile
    container_name: station-service
    depends_on:
      consul:
        condition: service_started
        required: true
      sqlserver:
        condition: service_started
        required: true
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      CONSUL_HTTP_ADDR: http://consul:8500
      SERVICE_NAME: station-service
    networks:
      OurNetworks-App: null
    ports:
      - mode: ingress
        target: 5004
        published: "5004"
        protocol: tcp
networks:
  OurNetworks-App:
    name: dockercompose11862061094755103843_OurNetworks-App
    driver: bridge
volumes:
  sql_data:
    name: dockercompose11862061094755103843_sql_data